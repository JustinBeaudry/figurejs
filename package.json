	"@types/node": "10.12.x",
	"husky": "1.3.x",
	"jest": "23.6.x",
	"lint-staged": "8.1.x",
	"prettier": "1.15.x",
	"ts-jest": "23.10.x",
	"ts-node": "8.0.x",
	"tslib": "1.9.x",
	"tslint": "5.12.x",
	"tslint-config-prettier": "1.17.x",
	"typedoc": "0.14.2",
	"typescript": "3.2.x"
 },
 "scripts": {
	"clean": "rm -rf lib && rm -rf coverage",
